<script>
    // Dashboard toggle functionality
    document.addEventListener('DOMContentLoaded', function() {
        const toggle = document.getElementById('dashboard-toggle');
        const toggleButtons = toggle ? toggle.querySelectorAll('.toggle-btn') : [];
        const dashboardContents = document.querySelectorAll('.dashboard-content');
        
        function switchDashboard(dashboardType) {
            // Update toggle buttons
            toggleButtons.forEach(btn => {
                if (btn.getAttribute('data-dashboard') === dashboardType) {
                    btn.classList.add('active');
                } else {
                    btn.classList.remove('active');
                }
            });
            
            // Show/hide dashboard contents
            dashboardContents.forEach(content => {
                if (content.id === `dashboard-${dashboardType}`) {
                    content.style.display = 'block';
                } else {
                    content.style.display = 'none';
                }
            });
        }
        
        // Add click handlers to toggle buttons
        toggleButtons.forEach(btn => {
            btn.addEventListener('click', function() {
                const dashboardType = this.getAttribute('data-dashboard');
                switchDashboard(dashboardType);
            });
        });
        
        // Initialize with Event Analytics shown
        switchDashboard('events');
    });
</script>
